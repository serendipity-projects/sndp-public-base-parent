<project xmlns="http://maven.apache.org/DECORATION/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Apache Commons" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd">
<!-- N.B. all non-body elements are automatically inherited -->
<bannerLeft>
<name>Serendipity Open Source Projects</name>

<src>images/opensourceproject_logo.jpeg</src>
